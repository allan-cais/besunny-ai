# BeSunny.ai Environment Configuration
# Copy this file to .env.local and fill in your values

# Application Environment
VITE_APP_ENV=development

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Python Backend API
VITE_API_URL=http://localhost:8000

# Google OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your-google-client-id
VITE_GOOGLE_CLIENT_SECRET=your-google-client-secret

# OpenAI Configuration
VITE_OPENAI_API_KEY=your-openai-api-key

# Feature Flags
VITE_ENABLE_AI_CLASSIFICATION=true
VITE_ENABLE_REALTIME=true
VITE_ENABLE_ANALYTICS=true

# Development Settings
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_WEBHOOK_TESTING=true

# External Services
VITE_N8N_WEBHOOK_URL=your-n8n-webhook-url
VITE_PINECONE_API_KEY=your-pinecone-api-key
VITE_PINECONE_ENVIRONMENT=your-pinecone-environment

# Monitoring and Analytics
VITE_SENTRY_DSN=your-sentry-dsn
VITE_ANALYTICS_ID=your-analytics-id

# Security
VITE_ENABLE_HTTPS_ONLY=false
VITE_ENABLE_STRICT_CSP=false
